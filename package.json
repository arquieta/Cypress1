  "keywords": [
    "automation",
    "testing",
    "cypress"
  ],
  "devDependencies": {
    "cypress": "^12.13.0",
    "cypress-mochawesome-reporter": "^3.5.1"
  },
  "dependencies": {
    "cypress-cucumber-preprocessor": "^4.3.1",
    "prompt-sync": "^4.2.0",
    "typescript": "^4.9.5"
  }
}
